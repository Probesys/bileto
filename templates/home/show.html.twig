{% extends 'base.html.twig' %}

{% block body %}
    <p>
        {{ 'home.show.hello' | trans }}
    </p>

    <form action="{{ path('logout') }}" method="post">
        <input type="hidden" name="_csrf_token" value="{{ csrf_token('logout') }}">

        <button id="form-logout-submit" type="submit">
            {{ 'home.show.logout' | trans }}
        </button>
    </form>
{% endblock %}
